    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "standard-version": "^8.0.1",
    "storybook-readme": "^3.0.4",
    "styled-components": "^2.0.0"
