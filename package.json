    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.3",
    "jest-styled-components": "^4.4.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
